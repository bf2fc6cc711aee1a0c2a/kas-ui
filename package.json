{
  "name": "kas-ui",
  "federatedModuleName": "kas",
  "version": "0.0.2",
  "description": "The Control Plane UI for Red Hat Kafka Instances for Apache Kafka",
  "main": "server.js",
  "port": "1337",
  "repository": "https://github.com/bf2fc6cc711aee1a0c2a/mk-ui-frontend.git",
  "homepage": "https://cloud.redhat.com/application-services/openshift-streams",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "npm run clean",
    "dr:surge": "node dr-surge.js",
    "build": "webpack --config webpack.prod.js && npm run dr:surge",
    "start": "node server.js",
    "mock": "asapi",
    "start:dev": "webpack serve --https --hot --color --progress --config webpack.dev.js",
    "start:federate": "HOST=localhost PORT=9000 PROTOCOL=http webpack serve --hot --color --progress --config webpack.dev.js",
    "test": "jest",
    "eslint": "eslint --ext .tsx,.js ./src/",
    "lint:js": "eslint --ext .tsx ./src/",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint": "npm run eslint",
    "format": "prettier --check --write ./src/**/*.{tsx,ts}",
    "type-check": "tsc --noEmit",
    "ci-checks": "npm run type-check && npm run lint && npm run test",
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "npm run build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist",
    "i18n": "i18next 'src/**/**/*.{ts,tsx}' [-oc] -c './i18next-parser.config.js' -o './src/locales/$LOCALE/$NAMESPACE.json'",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@rhoas/api-mock": "0.1.2",
    "@testing-library/jest-dom": "5.13.0",
    "@testing-library/react": "11.2.7",
    "@testing-library/user-event": "13.1.9",
    "@types/enzyme": "3.10.8",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/jest": "26.0.23",
    "@types/react-router-dom": "5.1.7",
    "@types/victory": "33.1.4",
    "@typescript-eslint/eslint-plugin": "4.26.0",
    "@typescript-eslint/parser": "4.26.0",
    "copy-webpack-plugin": "9.0.0",
    "css-loader": "5.2.6",
    "dotenv-webpack": "7.0.3",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "enzyme-to-json": "3.6.2",
    "eslint": "7.28.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.3.1",
    "husky": "6.0.0",
    "imagemin": "8.0.0",
    "jest": "27.0.4",
    "mini-css-extract-plugin": "1.6.0",
    "null-loader": "4.0.1",
    "optimize-css-assets-webpack-plugin": "6.0.0",
    "prettier": "2.3.1",
    "prop-types": "15.7.2",
    "raw-loader": "4.0.2",
    "react-axe": "3.5.4",
    "react-docgen-typescript-loader": "3.7.2",
    "react-router-dom": "5.2.0",
    "regenerator-runtime": "0.13.7",
    "rimraf": "3.0.2",
    "style-loader": "2.0.0",
    "svg-url-loader": "7.1.1",
    "terser-webpack-plugin": "5.1.3",
    "ts-jest": "27.0.3",
    "ts-loader": "9.2.3",
    "tsconfig-paths-webpack-plugin": "3.5.1",
    "tslib": "2.2.0",
    "typescript": "4.3.2",
    "url-loader": "4.1.1",
    "webpack": "5.38.1",
    "webpack-bundle-analyzer": "4.4.2",
    "webpack-cli": "4.7.0",
    "webpack-dev-server": "3.11.2",
    "webpack-merge": "5.8.0"
  },
  "dependencies": {
    "@bf2/ui-shared": "0.1.2",
    "@cloudmosaic/quickstarts": "0.0.24",
    "@rhoas/kafka-management-sdk": "0.1.0",
    "date-fns": "2.22.1",
    "@patternfly/patternfly": "4.103.6",
    "@patternfly/react-core": "4.121.1",
    "@patternfly/react-icons": "4.10.7",
    "@patternfly/react-styles": "4.10.7",
    "@patternfly/react-table": "4.27.7",
    "@redhat-cloud-services/frontend-components": "3.2.3",
    "@redhat-cloud-services/frontend-components-config": "4.1.9",
    "axios": "0.21.1",
    "dayjs": "1.10.5",
    "express": "4.17.1",
    "i18next": "20.3.1",
    "i18next-browser-languagedetector": "6.1.1",
    "i18next-http-backend": "1.2.6",
    "i18next-parser": "4.2.0",
    "i18next-pseudo": "2.2.1",
    "keycloak-js": "9.0.3",
    "msw": "0.29.0",
    "openapi-backend": "3.9.2",
    "path": "0.12.7",
    "postcss": "8.3.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-i18next": "11.10.0",
    "react-redux": "7.2.4",
    "react-router-last-location": "2.0.1",
    "redux": "4.1.0"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{tsx,ts}": "prettier --write",
    "*.tsx": "eslint --ext .tsx"
  }
}
