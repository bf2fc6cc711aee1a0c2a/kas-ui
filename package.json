{
  "name": "mk-ui-frontend",
  "federatedModuleName": "mkUiFrontend",
  "crc": {
    "name": "rhosak-control-plane-ui-build",
    "bundle": "application-services",
    "beta": true
  },
  "version": "0.0.2",
  "description": "The Control Plane UI for Red Hat OpenShift Streams for Apache Kafka",
  "main": "server.js",
  "port": "1337",
  "repository": "https://github.com/bf2fc6cc711aee1a0c2a/mk-ui-frontend.git",
  "homepage": "https://cloud.redhat.com/application-services/openshift-streams",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "npm run clean",
    "dr:surge": "node dr-surge.js",
    "build": "webpack --config webpack.prod.js && npm run dr:surge",
    "api:generate": "openapi-generator-cli generate -i mas-mock/managed-services-api.yaml -g typescript-axios -o src/openapi --additional-properties=ngVersion=6.1.7,npmName=restClient,supportsES6=true,npmVersion=6.9.0,withInterfaces=true",
    "start": "node server.js",
    "start:dev": "webpack serve --https --hot --color --progress --config webpack.dev.js",
    "start:federate": "HOST=localhost PORT=9000 PROTOCOL=http webpack serve --hot --color --progress --config webpack.dev.js",
    "test": "jest",
    "eslint": "eslint --ext .tsx,.js ./src/",
    "lint": "npm run eslint",
    "format": "prettier --check --write ./src/**/*.{tsx,ts}",
    "type-check": "tsc --noEmit",
    "ci-checks": "npm run type-check && npm run lint && npm run test",
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "npm run build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "i18n": "i18next 'src/**/**/*.{ts,tsx}' [-oc] -c './i18next-parser.config.js' -o './src/locales/$LOCALE/$NAMESPACE.json'"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "2.1.7",
    "@redhat-cloud-services/frontend-components-config": "^3.0.0-beta16",
    "@storybook/addon-actions": "^6.0.26",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/addons": "^6.0.26",
    "@storybook/react": "^6.0.26",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "@types/enzyme": "^3.10.7",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.14",
    "@types/react-router-dom": "^5.1.6",
    "@types/victory": "^33.1.4",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "^5.0.0",
    "dotenv-webpack": "^4.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.11.0",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "file-loader": "^6.1.1",
    "html-webpack-plugin": "^4.5.0",
    "imagemin": "^7.0.0",
    "jest": "^26.5.3",
    "mini-css-extract-plugin": "^1.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "prettier": "^2.1.2",
    "prop-types": "^15.6.1",
    "raw-loader": "^4.0.2",
    "react-axe": "^3.5.4",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^6.0.0",
    "terser-webpack-plugin": "^5.0.0",
    "ts-jest": "^26.4.1",
    "ts-loader": "^8.0.5",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "tslib": "^2.0.3",
    "typescript": "^4.0.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.1.3",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.2.0"
  },
  "dependencies": {
    "@patternfly/react-core": "^4.63.3",
    "@patternfly/react-icons": "^4.7.11",
    "@patternfly/react-styles": "^4.7.8",
    "@patternfly/react-table": "^4.19.24",
    "@redhat-cloud-services/frontend-components": "^2.4.22",
    "axios": "^0.21.0",
    "dayjs": "^1.9.6",
    "express": "^4.17.1",
    "i18next": "^19.8.3",
    "i18next-browser-languagedetector": "^6.0.1",
    "i18next-http-backend": "^1.0.21",
    "i18next-parser": "^3.3.0",
    "i18next-pseudo": "^2.2.0",
    "keycloak-js": "^9.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-i18next": "^11.7.3",
    "react-redux": "^7.2.2",
    "react-router-last-location": "^2.0.1",
    "redux": "^4.0.5"
  }
}
