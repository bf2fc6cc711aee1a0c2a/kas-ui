{
  "name": "kas-ui",
  "federatedModuleName": "kas",
  "version": "0.0.2",
  "description": "The Control Plane UI for Red Hat Kafka Instances for Apache Kafka",
  "main": "server.js",
  "port": "1337",
  "repository": "https://github.com/bf2fc6cc711aee1a0c2a/mk-ui-frontend.git",
  "homepage": "https://cloud.redhat.com/application-services/openshift-streams",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "npm run clean",
    "dr:surge": "node dr-surge.js",
    "build": "webpack --config webpack.prod.js && npm run dr:surge",
    "start": "node server.js",
    "mock": "asapi",
    "start:dev": "webpack serve --https --hot --color --progress --config webpack.dev.js",
    "start:federate": "HOST=localhost PORT=9000 PROTOCOL=http webpack serve --hot --color --progress --open=false --config webpack.dev.js",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "lint": "npm-run-all 'lint:src -- .' 'lint:format -- .'",
    "lint:src": "eslint --config ./linting/eslint.config.js --fix --max-warnings 74 --ignore-path ./linting/.eslintignore",
    "lint:styling": "stylelint --config ./linting/stylelint.config.js --fix --max-warnings 5",
    "lint:format": "prettier --config ./linting/prettier.config.js --write",
    "type-check": "tsc --noEmit",
    "ci-checks": "npm run type-check && npm run lint && npm run test",
    "bundle-profile:build": "webpack --config webpack.prod.js --profile --json > dist/stats.json",
    "bundle-profile:analyze": "npm run bundle-profile:build && webpack-bundle-analyzer ./dist/stats.json",
    "clean": "rimraf dist",
    "i18n": "i18next 'src/**/**/*.{ts,tsx}' [-oc] -c './i18next-parser.config.js' -o './src/locales/$LOCALE/$NAMESPACE.json'",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@redhat-cloud-services/frontend-components-config": "4.3.9",
    "@rhoas/api-mock": "0.12.3",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.3.0",
    "@types/enzyme": "3.10.9",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/jest": "27.0.2",
    "@types/react-router-dom": "5.3.1",
    "@types/victory": "33.1.5",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "copy-webpack-plugin": "9.0.1",
    "css-loader": "6.4.0",
    "css-minimizer-webpack-plugin": "3.1.1",
    "dotenv-webpack": "7.0.3",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "enzyme-to-json": "3.6.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-i18next": "5.1.2",
    "eslint-plugin-jest": "24.7.0",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.3.2",
    "husky": "7.0.2",
    "imagemin": "8.0.1",
    "jest": "27.2.5",
    "mini-css-extract-plugin": "2.4.2",
    "npm-run-all": "4.1.5",
    "null-loader": "4.0.1",
    "prettier": "2.4.1",
    "prop-types": "15.7.2",
    "raw-loader": "4.0.2",
    "react-axe": "3.5.4",
    "react-docgen-typescript-loader": "3.7.2",
    "regenerator-runtime": "0.13.9",
    "rimraf": "3.0.2",
    "style-loader": "3.3.0",
    "svg-url-loader": "7.1.1",
    "terser-webpack-plugin": "5.2.4",
    "ts-jest": "27.0.5",
    "ts-loader": "9.2.6",
    "tsconfig-paths-webpack-plugin": "3.5.1",
    "tslib": "2.3.1",
    "typescript": "4.4.4",
    "url-loader": "4.1.1",
    "webpack": "5.58.1",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.0",
    "webpack-dev-server": "4.3.1",
    "webpack-merge": "5.8.0"
  },
  "dependencies": {
    "@cloudmosaic/quickstarts": "0.0.24",
    "@patternfly/patternfly": "4.132.2",
    "@patternfly/quickstarts": "1.2.2",
    "@patternfly/react-charts": "6.15.20",
    "@patternfly/react-core": "4.152.4",
    "@patternfly/react-icons": "4.11.14",
    "@patternfly/react-styles": "4.11.13",
    "@patternfly/react-table": "4.29.58",
    "@redhat-cloud-services/frontend-components": "3.4.8",
    "@rhoas/app-services-ui-components": "1.1.1",
    "@rhoas/app-services-ui-shared": "0.15.2",
    "@rhoas/kafka-management-sdk": "0.12.3",
    "axios": "0.23.0",
    "byte-size": "8.1.0",
    "date-fns": "2.25.0",
    "dayjs": "1.10.7",
    "express": "4.17.1",
    "i18next": "21.3.0",
    "i18next-browser-languagedetector": "6.1.2",
    "i18next-http-backend": "1.3.1",
    "i18next-parser": "5.0.0",
    "i18next-pseudo": "2.2.1",
    "keycloak-js": "9.0.3",
    "path": "0.12.7",
    "postcss": "8.3.9",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-i18next": "11.12.0",
    "react-redux": "7.2.5",
    "react-router-dom": "5.2.1",
    "react-router-last-location": "2.0.1",
    "redux": "4.1.1"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{tsx,ts}": "prettier --write",
    "*.tsx": "eslint --ext .tsx"
  }
}
